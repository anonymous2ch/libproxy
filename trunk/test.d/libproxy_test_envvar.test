#!/bin/sh
# $Id$

result_ok="http://proxy:1234"
testurl="http://www.google.com"
proxy_bin="src/bin/proxy"

if [ -x $proxy_bin ]; then
    result=$(PX_CONFIG_ORDER=envvar http_proxy="http://proxy:1234" $proxy_bin $testurl)
else
    result="N/A"
fi

echo 'name="test_env_var1"'
if [ "$result" = "$result_ok" ]; then
  echo pass=True
else
  echo pass=False
  echo "reason='$proxy_bin returned $result instead of $result_ok'"
fi
